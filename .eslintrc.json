{
  "extends": ["eslint:recommended", "@eslint/js/recommended"],
  "rules": {
    "no-unused-vars": "warn",
    "react-refresh/only-export-components": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
